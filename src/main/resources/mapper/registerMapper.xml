<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.repository.LoginUserRepository">
	<insert id="registerUser" parameterType="com.example.demo.domain.AppUserDetails">
		<![CDATA[
       insert into m_user (user_id,password) values (#{user_id},#{password})
       ]]>
	</insert>

    <insert id=" registerUserRole"  parameterType="com.example.demo.domain.AppUserDetails">
     <![CDATA[
       insert into t_user_role (user_id) values (#{user_id})
     ]]>
    
    </insert>
</mapper>